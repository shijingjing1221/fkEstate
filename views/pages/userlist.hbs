<div class="container">
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <th>用户名</th>
            <th>角色</th>
            <th>注册时间</th>
            <th>登陆时间</th>
            <th>编辑</th>
        </tr>
        </thead>
        <tbody>
        {{#each users}}
            <tr>
                <th>{{name}}</th>
                <th>{{role}}</th>
                <th>{{meta.creatAt}}</th>
                <th>{{meta.updateAt}}</th>
                <th><button class="btn-primary">修改</button></th>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>
